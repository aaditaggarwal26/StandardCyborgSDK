{
  "name": "scsdk",
  "version": "1.0.0",
  "summary": "Standard Cyborg C++ Library for Data, I/O, Algos, and more",
  "homepage": "https://www.standardcyborg.com/",
  "license": {
    "type": "Proprietary",
    "text": "No Public Release Yet"
  },
  "authors": {
    "Standard Cyborg": "jeff@standardcyborg.com"
  },
  "platforms": {
    "ios": "13.0",
    "osx": "11.0"
  },
  "source": {
    "path": "file:///Users/aaditaggarwal/StandardCyborgSDK/scsdk/"
  },
  "source_files": [
    "c++/scsdk/**/*.{h,hpp,cc,cpp}"
  ],
  "public_header_files": [
    "c++/scsdk/**/*.{hpp,h}"
  ],
  "header_mappings_dir": "c++/scsdk",
  "dependencies": {
    "EigenCPPCocoa": [
      "~> 3.4.0"
    ]
  },
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "$(inherited) \"$(PODS_ROOT)/EigenCPPCocoa\" $(PODS_ROOT)/../deps/happly/ $(PODS_ROOT)/../deps/json/include/ $(PODS_ROOT)/../deps/tinygltf/ $(PODS_ROOT)/../deps/stb/ $(PODS_ROOT)/../deps/SparseICP/ $(PODS_ROOT)/../deps/nanoflann/include/ $(PODS_ROOT)/../deps/libigl/include/ ",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++17",
    "CLANG_CXX_LIBRARY": "libc++",
    "OTHER_CPLUSPLUSFLAGS": "$(inherited) -DFMT_HEADER_ONLY=1-DHAVE_CONFIG_H=1 -DHAVE_PTHREAD=1 -DGUID_LIBUUID "
  },
  "user_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "$(inherited) \"$(PODS_ROOT)/EigenCPPCocoa\" $(PODS_ROOT)/../deps/happly/ $(PODS_ROOT)/../deps/json/include/ $(PODS_ROOT)/../deps/tinygltf/ $(PODS_ROOT)/../deps/stb/ $(PODS_ROOT)/../deps/SparseICP/ $(PODS_ROOT)/../deps/nanoflann/include/ $(PODS_ROOT)/../deps/libigl/include/ "
  }
}
